<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddProductComputerIntelCPUActivity">

    <Button
        android:id="@+id/add_product_computer_intel_CPU_activity_select_image_button"
        android:layout_width="0dp"
        android:layout_height="150dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:background="@color/Default2"
        android:text="choose image"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/add_product_computer_intel_CPU_activity_select_image_button">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <EditText
                android:id="@+id/app_product_computer_CPU_activity_name_edit_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:background="@drawable/default_edit_text"
                android:ems="10"
                android:hint="Name"
                android:inputType="textPersonName"
                android:padding="10dp"
                android:textColor="@color/Default5"
                android:textColorHint="@color/Default3" />

            <EditText
                android:id="@+id/app_product_computer_CPU_activity_price_edit_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:background="@drawable/default_edit_text"
                android:ems="10"
                android:hint="Price"
                android:inputType="numberDecimal"
                android:padding="10dp"
                android:textColor="@color/Default5"
                android:textColorHint="@color/Default3" />

            <EditText
                android:id="@+id/app_product_computer_CPU_activity_quantity_edit_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:background="@drawable/default_edit_text"
                android:ems="10"
                android:hint="Quantity"
                android:inputType="number"
                android:padding="10dp"
                android:textColor="@color/Default5"
                android:textColorHint="@color/Default3" />

            <Spinner
                android:id="@+id/app_product_computer_CPU_activity_CPU_socket_type_spinner"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:background="@drawable/default_edit_text"
                android:padding="10dp" />

            <EditText
                android:id="@+id/app_product_computer_CPU_activity_number_of_cores_edit_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:background="@drawable/default_edit_text"
                android:ems="10"
                android:hint="Number of cores"
                android:inputType="number"
                android:padding="10dp"
                android:textColor="@color/Default5"
                android:textColorHint="@color/Default3" />

            <EditText
                android:id="@+id/app_product_computer_CPU_activity_core_clock_edit_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:background="@drawable/default_edit_text"
                android:ems="10"
                android:hint="Core clock"
                android:inputType="numberDecimal"
                android:padding="10dp"
                android:textColor="@color/Default5"
                android:textColorHint="@color/Default3" />

            <EditText
                android:id="@+id/app_product_computer_CPU_activity_boost_clock_edit_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:background="@drawable/default_edit_text"
                android:ems="10"
                android:hint="Boost clock"
                android:inputType="textPersonName|numberDecimal"
                android:padding="10dp"
                android:textColor="@color/Default5"
                android:textColorHint="@color/Default3" />

            <EditText
                android:id="@+id/app_product_computer_CPU_activity_TDP_edit_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:background="@drawable/default_edit_text"
                android:ems="10"
                android:inputType="numberDecimal"
                android:padding="10dp"
                android:hint="TDP"
                android:textColor="@color/Default5"
                android:textColorHint="@color/Default3" />

            <EditText
                android:id="@+id/app_product_computer_CPU_activity_integrated_graphics_card_edit_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:background="@drawable/default_edit_text"
                android:ems="10"
                android:hint="Integrated graphics card"
                android:inputType="textPersonName"
                android:padding="10dp"
                android:textColor="@color/Default5"
                android:textColorHint="@color/Default3" />

            <MultiAutoCompleteTextView
                android:id="@+id/app_product_computer_CPU_activity_description_edit_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="16dp"
                android:background="@drawable/default_edit_text"
                android:hint="Description"
                android:padding="10dp"
                android:textColor="@color/Default5"
                android:textColorHint="@color/Default3" />
        </LinearLayout>
    </ScrollView>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.9" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/app_product_computer_CPU_activity_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/profile_signin_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline5">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Add product"
            android:textColor="@color/Default_font_color_light"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>